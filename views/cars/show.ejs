            <% if (user) { %>
                <a class='allLinks' href='/logout'>Log Out</a>
                <span>Hello <%= user.name %> </span>
                <% } else { %>
                    <a class='allLinks' href='/auth/google'>Log In</a>
                    <% } %>
                </nav>
                <h1 class='title'>Detail Page</h1>
                <p class='title-description'>Nice Read Aint It?</p>
                <section>
                    <table class='key'>
                        <thread>
                            <tr>
                                <th>Make</th>
                                <th>Model</th>
                                <th>Year</th>
                                <th>Date Posted</th>
                                <th>Author</th>
                            </tr>
                        </thread>
                        <tbody>
                            
                        </tbody>
                    </table>
                </section>         
</body>
